(executable
 (name server)
 (libraries memoir_lib dream lwt)
 (modes byte exe))

(executable
 (name generator)
 (libraries
  memoir_lib
  js_of_ocaml
  tyxml
  omd
  str
  yaml
  core_unix
  bigstringaf
  threads
  memoir.types
  memoir.content
  memoir.templates)
 (modes native))

(rule
 (targets server.bc.js)
 (action
  (run %{bin:js_of_ocaml} --source-map %{dep:server.bc} -o %{targets})))
